{
  "name": "fresh-Inbox",
  "version": "1.0.0",
  "description": "Clean Inbox, Total Privacy: Fresh Inbox Delivers Both.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonghakseo/chrome-extension-boilerplate-react-vite.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build -w",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/chrome": "0.0.224",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.33.1",
    "fs-extra": "11.1.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "2.8.8",
    "rollup": "4.3.0",
    "sass": "^1.65.1",
    "tailwindcss": "^3.3.3",
    "ts-loader": "9.4.2",
    "typescript": "4.8.3",
    "vite": "5.0.0",
    "ws": "8.13.0"
  }
}
